<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="student" package="com.axelor.student.db" />

	<entity name="Address" cachable="true">
		<string name="street" required="true" max="255" initParam="true" />
		<string name="area" max="255" initParam="true" />
		<string name="city" initParam="true" />
		<string name="zip" initParam="true" />
		<string name="state" initParam="true" />
		<many-to-one name="country" ref="Country"/>
	</entity>

</domain-models>
