<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="student" package="com.axelor.student.db" />

	<entity name="Country" cachable="true">
		<string name="code" required="true" unique="true" min="2" initParam="true"/>
		<string name="name" required="true" unique="true" min="2" initParam="true"/>
		
		<finder-method name="findByName" using="name" />
		<finder-method name="findByCode" using="code" />
	</entity>

</domain-models>
